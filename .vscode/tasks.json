{
	"version": "2.0.0",
	"tasks": [
		{
			"command":"wasm-pack",
			"args": ["build", "--target", "web"],
			"problemMatcher": [
				"$rustc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"label": "wasm-pack build"
		},
		{
			"type": "cargo",
			"command": "run",
			"group": "build",
			"label": "rust: cargo run"
		}
		
	]
}